   "source": [
    "r_vals_all = np.geomspace(0.001,0.5,20)\n",
    "mean_vels = []\n",
    "\n",
    "center = np.array([0.4,0.22,1.3])\n",
    "t = 0.5\n",
    "\n",
    "for r in r_vals_all:\n",
    "\n",
    "    \n",
    "\n",
    "    n_points = 150\n",
    "    points = []\n",
    "    for n in range(n_points):\n",
    "        phi = np.random.uniform(0,2*np.pi)\n",
    "        costheta = np.random.uniform(-1,1)\n",
    "        u = np.random.uniform(0,1)\n",
    "\n",
    "        theta = np.arccos( costheta )\n",
    "        r_vals = r * u**(1./3)\n",
    "\n",
    "        x = r_vals * np.sin( theta) * np.cos( phi )\n",
    "        y = r_vals * np.sin( theta) * np.sin( phi )\n",
    "        z = r_vals * np.cos( theta)\n",
    "\n",
    "        points.append([x,y,z])\n",
    "\n",
    "    #print(points)\n",
    "    points = np.array(points) + center\n",
    "    vels = interface.get_velocity(t,points)\n",
    "    mean_vels.append(np.mean(vels,axis=0))\n",
    "    \n",
    "print(np.shape(mean_vels))\n",
    "print(np.shape(r_vals_all))\n",
    "mean_vels = np.array(mean_vels)"
   ]
